<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="analysisVerifier" language="groovy" pageWidth="100" pageHeight="572" orientation="Landscape" columnWidth="100" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="4.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ANALYSIS_ID" class="java.lang.Integer"/>
	<parameter name="SYSTEM_NAME" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
      first 1 au.*, s.*
FROM
      security_dev:system_user s, analysis_user au
WHERE
      s.id = au.system_user_id AND au.analysis_id  = $P{ANALYSIS_ID}
     AND au.action_id IN (select id from dictionary d where d.system_name = $P{SYSTEM_NAME})]]>
	</queryString>
	<field name="id" class="java.lang.Integer"/>
	<field name="initials" class="java.lang.String"/>
	<detail>
		<band height="12" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="100" height="12"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{initials}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
