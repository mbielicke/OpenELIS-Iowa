	<queryString>
		<![CDATA[SELECT DISTINCT
     s.id s_id, s.domain, s.accession_number, s.revision, s.order_id, s.collection_date, s.collection_time, s.received_date,
     d.entry s_status, s.client_reference
FROM
     history h, sample s, dictionary d
WHERE
     d.id = s.status_id AND
     h.reference_id = s.id  AND h.reference_table_id = $P{REFERENCE_TABLE_ID}
     AND h.timestamp BETWEEN $P{BEGIN_ENTERED} AND $P{END_ENTERED}
     AND h.activity_id IN (1,2) $P!{USER_WHERE}
ORDER BY
     s.accession_number]]>
	</queryString>
