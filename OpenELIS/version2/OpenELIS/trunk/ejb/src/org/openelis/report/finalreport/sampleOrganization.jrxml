<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sampleOrganization" language="groovy" pageWidth="210" pageHeight="572" orientation="Landscape" columnWidth="210" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="3.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="SAMPLE_ORGANIZATION_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     organization.name organization_name,
     organization_address.multiple_unit organization_address_multiple_unit,
     organization_address.street_address organization_address_street_address,
     organization_address.city organization_address_city,
     organization_address.zip_code organization_address_zip_code,
     organization_address.state organization_address_state
FROM
     sample_organization sample_organization,
     organization organization,
     address organization_address
WHERE
     sample_organization.id = $P{SAMPLE_ORGANIZATION_ID} AND organization.id = sample_organization.organization_id AND organization_address.id = organization.address_id]]>
	</queryString>
	<field name="organization_name" class="java.lang.String"/>
	<field name="organization_address_multiple_unit" class="java.lang.String"/>
	<field name="organization_address_street_address" class="java.lang.String"/>
	<field name="organization_address_city" class="java.lang.String"/>
	<field name="organization_address_zip_code" class="java.lang.String"/>
	<field name="organization_address_state" class="java.lang.String"/>
	<detail>
		<band height="48" splitType="Stretch">
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="0" width="210" height="12"/>
				<textElement>
					<font fontName="Times New Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{organization_name}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="24" width="210" height="12"/>
				<textElement>
					<font fontName="Times New Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{organization_address_street_address}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="36" width="210" height="12"/>
				<textElement>
					<font fontName="Times New Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[org.openelis.report.util.Concat.concatWithSeparator(org.openelis.report.util.Concat.concatWithSeparator($F{organization_address_city}," ",$F{organization_address_state}), " ", $F{organization_address_zip_code})]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement positionType="Float" x="0" y="12" width="210" height="12" isRemoveLineWhenBlank="true"/>
				<textElement>
					<font fontName="Times New Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{organization_address_multiple_unit}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
