	<queryString>
		<![CDATA[SELECT s.accession_number, s.collection_date, s.collection_time, s.received_date, s.domain,
       se.priority, t.name t_name, t.time_ta_average, t.time_holding,
       m.name m_name, d.entry status, d1.entry sample_type, a.id a_id
FROM sample s, sample_item si, sample_environmental se, analysis a,
     test t, method m, section sec, dictionary d, dictionary d1,
     sample_organization sd
WHERE s.domain = "E" and
      s.entered_date between $P{FROM} and $P{TO} and
      s.id = si.sample_id and s.id = se.sample_id and
      si.id = a.sample_item_id and a.test_id = t.id and
      t.method_id = m.id and a.section_id = sec.id and
      a.status_id = d.id and si.type_of_sample_id = d1.id and
      s.id = sd.sample_id
      $P!{STATUS} $P!{ORG_ID} $P!{TEST} $P!{SECTION} $P!{PROJECT}

UNION

SELECT s.accession_number, s.collection_date, s.collection_time, s.received_date, s.domain,
       cast(null as integer), t.name t_name, t.time_ta_average, t.time_holding,
       m.name m_name, d.entry status, d1.entry sample_type, a.id a_id
FROM sample s, sample_item si, sample_private_well sd, analysis a,
     test t, method m, section sec, dictionary d, dictionary d1
WHERE s.domain = 'W' and
      s.entered_date between $P{FROM} and $P{TO} and
      s.id = si.sample_id and s.id = sd.sample_id and
      si.id = a.sample_item_id and a.test_id = t.id and
      t.method_id = m.id and a.section_id = sec.id and
      a.status_id = d.id and si.type_of_sample_id = d1.id
      $P!{STATUS} $P!{ORG_ID} $P!{TEST} $P!{SECTION} $P!{PROJECT}

UNION

SELECT s.accession_number, s.collection_date, s.collection_time, s.received_date, s.domain,
       cast(null as integer), t.name t_name, t.time_ta_average, t.time_holding,
       m.name m_name, d.entry status, d1.entry sample_type, a.id a_id
FROM sample s, sample_item si, analysis a,
     test t, method m, section sec, dictionary d, dictionary d1,
     sample_organization sd
WHERE s.domain not in ('E', 'W') and
      s.entered_date between $P{FROM} and $P{TO} and
      s.id = si.sample_id and
      si.id = a.sample_item_id and a.test_id = t.id and
      t.method_id = m.id and a.section_id = sec.id and
      a.status_id = d.id and  si.type_of_sample_id = d1.id and
      s.id = sd.sample_id
      $P!{STATUS} $P!{ORG_ID} $P!{TEST} $P!{SECTION} $P!{PROJECT}

ORDER BY $P!{ORDER_BY}]]>
	</queryString>
