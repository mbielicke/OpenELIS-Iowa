                <ui:text from="{msg.pws_id}:" />
              </td>
              <td>
                <g:HorizontalPanel>
                  <w:TextBox ui:field="sdwisPWSNumber0" width="75px" case="UPPER" field="String"
                    maxLength="9" />
                  <g:Image ui:field="sdwisPWSNumber0IM" />
                </g:HorizontalPanel>
              </td>
              <td>
                <g:Image ui:field="sdwisPWSNumber0IC" />
