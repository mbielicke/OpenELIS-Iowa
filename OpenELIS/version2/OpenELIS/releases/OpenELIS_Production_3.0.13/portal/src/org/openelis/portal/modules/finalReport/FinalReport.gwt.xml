<module>
  
  <replace-with class="org.openelis.portal.modules.finalReport.client.FinalReportUIMobileImpl">
    <when-type-assignable class="org.openelis.portal.modules.finalReport.client.FinalReportUI"/>
    <when-property-is name="platform" value="phone"/>
  </replace-with>
  
  <replace-with class="org.openelis.portal.modules.finalReport.client.FinalReportUITabletImpl">
    <when-type-assignable class="org.openelis.portal.modules.finalReport.client.FinalReportUI"/>
    <when-property-is name="platform" value="tablet"/>
  </replace-with>
  

</module>