	<queryString>
		<![CDATA[SELECT
     w.description as worksheet_description, w.format_id, wi.position, wav.*, d1.entry status
FROM
     worksheet_analysis_view wav
     join worksheet w on wav.worksheet_id = w.id
     join worksheet_item wi on wav.worksheet_item_id = wi.id
     left join dictionary d1 on wav.status_id = d1.id
WHERE
     wav.worksheet_id = $P{WORKSHEET_ID}
ORDER BY
     wi.position, wav.id]]>
	</queryString>
