<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
	xmlns:r='urn:import:org.openelis.ui.widget.tree' xmlns:t='urn:import:org.openelis.ui.widget.table'>

	<ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
	<ui:with type="org.openelis.modules.main.client.resources.OpenELISResources"
		field="res" />

	<g:SplitLayoutPanel styleName="{res.style.WhiteContentPanel}">
		<g:center>
			<g:LayoutPanel>
				<g:layer top="0px" height="30px">
					<g:AbsolutePanel styleName="{res.buttonPanel.ButtonPanelContainer}"
						width="100%">
						<g:HorizontalPanel>
							<w:Button css="{res.buttonPanel}" ui:field="query"
								leftIcon="{res.icon.QueryButtonImage}" text="{msg.gen_query}"
								toggles="true" />
							<g:AbsolutePanel styleName="{res.buttonPanel.ButtonDivider}" />
							<w:Button css="{res.buttonPanel}" ui:field="commit"
								leftIcon="{res.icon.CommitButtonImage}" text="{msg.gen_commit}" />
							<w:Button css="{res.buttonPanel}" ui:field="abort"
								leftIcon="{res.icon.AbortButtonImage}" text="{msg.gen_abort}" />
							<g:AbsolutePanel styleName="{res.buttonPanel.ButtonDivider}" />
							<w:Menu ui:field="optionsMenu" selfShow="true">
								<w:display>
									<w:Button ui:field="optionsButton" css="{res.buttonPanel}"
										text="{msg.gen_options}" rightIcon="{res.icon.OptionsButtonImage}" />
								</w:display>
							</w:Menu>
						</g:HorizontalPanel>
					</g:AbsolutePanel>
				</g:layer>
				<g:layer top="30px" height="100px">
					<g:HTMLPanel>
						<table class="{res.style.Form}">
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.sample_accessionNum}:" />
								</td>
								<td>
									<w:TextBox ui:field="id" width="90px" field="Integer" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_printer}:" />
								</td>
								<td>
									<w:Dropdown ui:field="printer" width="90px" field="Integer" />
								</td>
							</tr>
							<tr>
								<td>
									<g:HorizontalPanel>
										<w:Button ui:field="run" leftIcon=""
											text="{msg.report_runReport}" />
									</g:HorizontalPanel>
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</g:layer>
				<g:layer bottom="35px" height="150px">
					<r:Tree ui:field="tree" header="true"
						styleName="{res.table.ScreenTableWithSides}"
						allowMultipleSelection="true">
						<r:Column width="100" label="worksheet id" />
						<r:Column width="200" label="worksheet description" />
						<r:Column width="100" label="worksheet date" />
						<r:Column width="200" label="worksheet instrument" />
						<r:Leaf key="qc">
							<r:LeafColumn>
								<t:CheckBoxCell>
									<w:CheckBox />
								</t:CheckBoxCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:TextBoxCell>
									<w:TextBox field="Integer" />
								</t:TextBoxCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:LabelCell>
									<w:Label />
								</t:LabelCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:LabelCell>
									<w:Label />
								</t:LabelCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:LabelCell>
									<w:Label />
								</t:LabelCell>
							</r:LeafColumn>
						</r:Leaf>
						<r:Leaf key="worksheet">
							<r:LeafColumn>
								<t:TextBoxCell>
									<w:TextBox field="Integer" />
								</t:TextBoxCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:TextBoxCell>
									<w:TextBox field="String" />
								</t:TextBoxCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:TextBoxCell>
									<w:TextBox field="Datetime" />
								</t:TextBoxCell>
							</r:LeafColumn>
							<r:LeafColumn>
								<t:TextBoxCell>
									<w:TextBox field="String" />
								</t:TextBoxCell>
							</r:LeafColumn>
						</r:Leaf>
					</r:Tree>
				</g:layer>
			</g:LayoutPanel>
		</g:center>
	</g:SplitLayoutPanel>
</ui:UiBinder>