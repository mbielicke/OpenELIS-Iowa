	<queryString>
		<![CDATA[SELECT s.id s_id, s.accession_number, s.collection_date, s.collection_time,
       t.name t_name, m.name m_name, d.entry status, d1.entry sample_type, a.id a_id,
       t1.name t1_name, m1.name m1_name, d2.entry status1
  FROM sample s, sample_item si, analysis a,
       test t, method m, section sec, dictionary d, dictionary d1,
       outer(analysis a1, test t1, method m1, dictionary d2)
 WHERE s.entered_date between $P{FROM} and $P{TO} and
       s.id = si.sample_id and
       si.id = a.sample_item_id and a.test_id = t.id and
       t.method_id = m.id and a.section_id = sec.id and
       a.status_id = d.id and si.type_of_sample_id = d1.id and
       a.pre_analysis_id = a1.id and a1.test_id = t1.id and t1.method_id = m1.id and a1.status_id = d2.id and
       $P{PREP_TEST} = ""
       $P!{STATUS} $P!{TEST} $P!{SECTION}
UNION
SELECT s.id s_id, s.accession_number, s.collection_date, s.collection_time,
       t.name t_name, m.name m_name, d.entry status, d1.entry sample_type,
       a.id a_id, t1.name t1_name, m1.name m1_name, d2.entry status1
  FROM sample s, sample_item si, analysis a,
       test t, method m, section sec, dictionary d, dictionary d1,
       analysis a1, test t1, method m1, dictionary d2
 WHERE s.entered_date between $P{FROM} and $P{TO} and
       s.id = si.sample_id and
       si.id = a.sample_item_id and a.test_id = t.id and
       t.method_id = m.id and a.section_id = sec.id and
       a.status_id = d.id and si.type_of_sample_id = d1.id and
       a.pre_analysis_id = a1.id and a1.test_id = t1.id and t1.method_id = m1.id and a1.status_id = d2.id
       $P!{STATUS} $P!{TEST} $P!{PREP_TEST} $P!{SECTION}
ORDER BY $P!{ORDER_BY}]]>
	</queryString>
