<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:w='urn:import:org.openelis.ui.widget' xmlns:t='urn:import:org.openelis.ui.widget.table'
             xmlns:c="urn:import:org.openelis.ui.widget.calendar" xmlns:b="urn:import:org.openelis.modules.worksheetBuilder.client">

  <ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
  <ui:with type="org.openelis.ui.resources.UIResources" field="uiRes" />
  <ui:with type="org.openelis.modules.main.client.resources.OpenELISResources" field="res" />

  <g:LayoutPanel>
    <g:layer top="0px" bottom="30px">
      <t:Table ui:field="worksheetItemTable" visibleRows="9" horizontalScroll="ALWAYS"
               verticalScroll="ALWAYS" header="true" styleName="{uiRes.table.ScreenTableWithSides}">
        <t:Column width="50" label="{msg.position}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="90" sortable="false" label="{msg.accessionNum}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="110" sortable="false" label="{msg.description}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="70" label="{msg.qcLink}">
          <t:DropdownCell>
            <w:Dropdown ui:field="qcLink" width="70" field="Integer"/>
          </t:DropdownCell>
        </t:Column>
        <t:Column width="100" sortable="false" label="{msg.test}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="100" sortable="false" label="{msg.method}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="100" label="{msg.unit}">
          <t:LabelCell/>
<!--
          <t:AutoCompleteCell>
            <w:AutoComplete ui:field="unitOfMeasureId" width="100" case="MIXED">
              <w:popup>
                <t:Table visibleRows="10" header="true">
                  <t:Column width="100" label="{msg.unit}">
                    <t:LabelCell/>
                  </t:Column>
                </t:Table>
              </w:popup>
            </w:AutoComplete>
          </t:AutoCompleteCell>
-->
        </t:Column>
        <t:Column width="75" label="{msg.status}">
          <t:DropdownCell>
            <w:Dropdown ui:field="analysisStatusId" width="75" case="MIXED"/>
          </t:DropdownCell>
        </t:Column>
        <t:Column width="75" label="{msg.collected}">
          <t:CalendarCell>
            <c:Calendar width="75px" begin="0" end="2"/> <!-- pattern="{msg.datePattern}" maxValue="0" -->
          </t:CalendarCell>
        </t:Column>
        <t:Column width="110" label="{msg.received}">
          <t:CalendarCell>
            <c:Calendar width="110px" begin="0" end="4"/> <!-- pattern="{msg.dateTimePattern}" maxValue="0" -->
          </t:CalendarCell>
        </t:Column>
        <t:Column width="50" label="{msg.due}">
          <t:LabelCell/>
        </t:Column>
        <t:Column width="110" label="{msg.expire}">
          <t:LabelCell/>
        </t:Column>
      </t:Table>
    </g:layer>
    <g:layer bottom="0px" height="30px">
      <g:HorizontalPanel>
        <w:Menu ui:field="addRowMenu" selfShow="true">
          <w:display>
            <w:Button ui:field="addRowButton" text="{msg.addRow}" leftIcon="{res.style.addRowIcon}"/>
          </w:display>
          <w:MenuItem ui:field="insertAnalysisAbove" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertAnalysisAbove}"/>
          <w:MenuItem ui:field="insertAnalysisBelow" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertAnalysisBelow}"/>
          <w:MenuItem ui:field="insertFromWorksheetAbove" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertFromWorksheetAbove}"/>
          <w:MenuItem ui:field="insertFromWorksheetBelow" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertFromWorksheetBelow}"/>
          <w:MenuItem ui:field="insertFromQcTableAbove" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertFromQcTableAbove}"/>
          <w:MenuItem ui:field="insertFromQcTableBelow" description=""
                      enabled="false" icon="{res.style.addRowIcon}"
                      display="{msg.insertFromQcTableBelow}"/>
        </w:Menu>
        <w:Button ui:field="removeRowButton" leftIcon="{res.style.removeRowIcon}"
                  text="{msg.removeRow}"/>
        <w:Menu ui:field="loadTemplateMenu" selfShow="true">
          <w:display>
            <w:Button ui:field="loadTemplateButton" text="{msg.loadTemplate}"
                      leftIcon="{res.style.addRowIcon}"/>
          </w:display>
        </w:Menu>
        <w:Menu ui:field="undoQcsMenu" selfShow="true">
          <w:display>
            <w:Button ui:field="undoQcsButton" text="{msg.undoQc}" leftIcon="{res.style.removeRowIcon}"/>
          </w:display>
          <w:MenuItem ui:field="undoAll" description="" enabled="false"
                      icon="{res.style.removeRowIcon}" display="All"/>
          <w:MenuItem ui:field="undoTemplate" description="" enabled="false"
                      icon="{res.style.removeRowIcon}" display="Template"/>
          <w:MenuItem ui:field="undoManual" description="" enabled="false"
                      icon="{res.style.removeRowIcon}" display="Manual"/>
        </w:Menu>
      </g:HorizontalPanel>
    </g:layer>
  </g:LayoutPanel>
</ui:UiBinder>