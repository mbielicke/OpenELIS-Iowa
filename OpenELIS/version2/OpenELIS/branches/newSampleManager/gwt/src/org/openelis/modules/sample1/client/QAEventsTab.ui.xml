<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
  xmlns:r='urn:import:org.openelis.ui.widget.tree' xmlns:t='urn:import:org.openelis.ui.widget.table' xmlns:c="urn:import:org.openelis.ui.widget.calendar"
  xmlns:s="urn:import:org.openelis.modules.sample1.client">

  <ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
  <ui:with type="org.openelis.ui.resources.UIResources" field="uiRes" />
  <ui:with type="org.openelis.modules.main.client.resources.OpenELISResources" field="res" />

  <g:LayoutPanel>
    <g:layer top="0px" bottom="25px">
     <g:HorizontalPanel padding="0" spacing="0">
       <g:VerticalPanel>
        <t:Table ui:field="sampleQATable" visibleRows="6" class="{res.type.ScreenTableWithSides}" verticalScroll="ALWAYS" horizontalScroll="ALWAYS" header="true">
              <t:Column width="172" label="{msg.sampleQAEvent}">
                <t:LabelCell>
                  <t:LabelCell field="Integer" ui:field = "ampleSubQaName"/>
                </t:Label>
              </t:Column>
              <t:Column width="90" label="{msg.type}">
                <t:DropdownCell>
                  <w:Dropdown width="90" ui:field = "sampleQaTypeId"/>
                </t:DropdownCell>
              </t:Column>
              <t:Column width="61" label="{msg.billable}">
                <t:CheckCell>
                  <t:Check ui:field = "sampleQaIsBillable"/>
                </t:CheckCell>
              </t:Column>
         </t:Table>
          <g:HorizontalPanel style="{res.type.TableButtonFooter}">
            <w:Button ui:field="removeSampleQAButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" /> 
            <w:Button ui:field="sampleQAPicker" leftIcon="{res.style.PickerButtonImage}" text="{msg.qaEvents}" /> 
          </g:HorizontalPanel>
        <VerticalPanel height = "5"/>
        <table class="{res.style.TabSubForm}">
          <tr>
            <td class="{res.style.WarningLabel}" ui:field="sampleBillableLabel"  width = "320" wordwrap = "true"/>
          </tr>
        </table>
      </g:VerticalPanel>
      <g:HorizontalPanel width="10" />
      <g:VerticalPanel>
        <t:Table ui:field="analysisQATable" visibleRows="6" class="{res.type.ScreenTableWithSides}" verticalScroll="ALWAYS" horizontalScroll="ALWAYS" header="true">
              <t:Column width="172" label="{msg.analysisQAEvent}">
                <t:LabelCell>
                  <t:Label field="Integer" ui:field="analysisSubQaName"/>
                </t:LabelCell>
              </t:Column>
              <t:Column width="90" label="{msg.type}">
                <t:DropdownCell>
                  <w:Dropdown width="90" ui:field="analysisQaTypeId"/>
                </t:DropdownCell>
              </t:Column>
              <t:Column width="61" label="{msg.billable}">
                <t:CheckCell>
                  <t:Check ui:field="analysisQaIsBillable"/>
                </t:CheckCell>
              </t:Column>
         </t:Table>
          <g:HorizontalPanel style="{res.type.TableButtonFooter}">
            <w:Button ui:field="removeAnalysisQAButton" leftIcon="{res.style.RemoveRowButtonImage}" text="{msg.removeRow}" /> 
            <w:Button ui:field="analysisQAPicker" leftIcon="{res.style.PickerButtonImage}" text="{msg.qaEvents}" /> 
          </g:HorizontalPanel>
        <g:VerticalPanel height = "5"/>
        <table class="{res.style.TabSubForm}">
          <tr>
            <td class="{res.style.WarningLabel}" ui:field="analysisBillableLabel" width = "330" wordwrap = "true"/>
          </tr>
        </table>
      </g:VerticalPanel>
    </g:HorizontalPanel>
   </g:layer>
 </g:LayoutPanel>
</ui:UiBinder>