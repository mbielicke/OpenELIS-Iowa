<web-app>
   <filter>
        <filter-name>Login Filter</filter-name>
        <filter-class>org.openelis.server.StaticFilter</filter-class>
        <init-param>
            <param-name>AppRoot</param-name>
            <param-value>/home/tschmidt/workspace/OpenELIS-GWT/src/org/openelis/public/</param-value>
        </init-param>
        <init-param>
            <param-name>ipTrys</param-name>
            <param-value>9</param-value>
        </init-param>
        <init-param>
            <param-name>userTrys</param-name>
            <param-value>3</param-value>
        </init-param>
    </filter>
 
  <filter-mapping>
    <filter-name>Login Filter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
	<servlet>
		<servlet-name>shell</servlet-name>
		<servlet-class>com.google.gwt.dev.shell.GWTShellServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>shell</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

</web-app>