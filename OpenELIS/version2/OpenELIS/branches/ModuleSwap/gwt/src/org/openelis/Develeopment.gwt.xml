<!--
   This module is used to compile a single permutation to be used for 
   development and testing purposes.  
 -->
<module rename-to="openelis">
    <!--  Inherit base app module -->
    <inherits name="org.openelis.OpenELIS"/>
    
    <!--  Inherit this module to use DebugIds for WebDriver -->
    <inherits name="com.google.gwt.user.Debug"/>
    
    <!--  Inherit Screen modules to compile for development -->
    <inherits name="org.openelis.modules.Screens"/>
    
    <!--  Only compile single browser permutation -->
    <set-property name="user.agent" value="gecko1_8" />
    
    <!--  Leaves CSS injections in human readable for testing and developing -->
    <set-configuration-property name="CssResource.style" value="pretty"/>

</module>