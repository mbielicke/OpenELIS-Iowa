#!/bin/sh
APPDIR=`dirname $0`;
TOOLS='/home/tschmidt/workspace/OpenELIS-Lib-tools/gwt-linux-UHL.1.5';
java -Xmx512m -cp "$APPDIR/src:$APPDIR/bin:$TOOLS/gwt-user.jar:$TOOLS/gwt-dev-linux.jar:../OpenELIS-Lib/openelis-lib.jar" com.google.gwt.dev.GWTCompiler -out "$APPDIR/www" "$@" org.openelis.OpenELIS;
