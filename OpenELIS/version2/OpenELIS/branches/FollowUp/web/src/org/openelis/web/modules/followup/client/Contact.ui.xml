<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
  xmlns:t='urn:import:org.openelis.ui.widget.table' xmlns:c='urn:import:org.openelis.ui.widget.calendar' xmlns:r='urn:import:org.openelis.ui.widget.tree'
  xmlns:l='urn:import:org.openelis.ui.widget.columnar' xmlns:f='urn:import:org.openelis.web.modules.followup.client'>


  <ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
  <ui:with type="org.openelis.ui.resources.UIResources" field="uiRes" />
  <ui:with type="org.openelis.web.modules.main.client.resources.Resources" field="res" />

  <g:LayoutPanel styleName="{res.style.ContentPanel}">
    <g:layer>
      <g:HTMLPanel>
        <table class="{res.style.Form}">
          <tr>
            <td class="{res.style.Prompt}">By:</td>
            <td>
              <w:TextBox ui:field="by"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">In Reference:</td>
            <td>
              <w:TextBox ui:field="inReference"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">Initiated By:</td>
            <td>
              <w:TextBox ui:field="initiatedBy"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">Organization:</td>
            <td>
              <w:AutoComplete ui:field="organiation"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">Person Contacted:</td>
            <td>
              <w:Dropdown ui:field="personContacted"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">Contact Datetime:</td>
            <td>
              <c:Calendar ui:field="contactTime" begin="0" end="4"/>
            </td>
          </tr>
          <tr>
            <td>
              <w:Dropdown ui:field="contactMethod"/>
            </td>
            <td>
              <w:TextBox ui:field="contactDetails"/>
            </td>
          </tr>
          <tr>
            <td class="{res.style.Prompt}">Comment:</td>
            <td>
              <w:TextArea ui:field="comments"/>
            </td>
          </tr>
        </table>
      </g:HTMLPanel>
    </g:layer>
  </g:LayoutPanel>
 
</ui:UiBinder>