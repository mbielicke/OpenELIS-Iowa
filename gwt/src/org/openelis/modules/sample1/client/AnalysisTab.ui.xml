<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
  xmlns:r='urn:import:org.openelis.ui.widget.tree' xmlns:t='urn:import:org.openelis.ui.widget.table' xmlns:c="urn:import:org.openelis.ui.widget.calendar"
  xmlns:s="urn:import:org.openelis.modules.sample1.client">

  <ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
  <ui:with type="org.openelis.ui.resources.UIResources" field="uiRes" />
  <ui:with type="org.openelis.modules.main.client.resources.OpenELISResources" field="res" />

  <g:LayoutPanel>
    <g:layer top="0px" bottom="25px">
    <g:VerticalPanel padding="0" spacing="0">
      <table class="{res.style.Form}">
        <tr>
          <td class="{res.style.Prompt}"><ui:text from="{msg.test}" /></td>
          <td>
             <w:AutoComplete ui:field="analysisTestName" width="150px" case="LOWER">
               <w:popup>
                 <t:Table visibleRows="10" header="true">
                   <t:Column width="170px" label="{msg.test}">
                     <t:LabelCell />
                   </t:Column>
                   <t:Column width="150px" label="{msg.method}">
                     <t:LabelCell />
                   </t:Column>
                   <t:Column width="290px" label="{msg.description}">
                     <t:LabelCell />
                   </t:Column>
                 </t:Table>
               </w:popup>
             </w:AutoComplete>
           </td>
          <td class="{res.style.Prompt}"><ui:text from="{msg.method}" /></td>
          <td colspan="3">         
             <w:Textbox ui:field="analysisMethodName" field = "String" width="151px" case="LOWER"/>
          </td>
          <td class="{res.style.Prompt}"><ui:text from="{msg.started}" /></td>
          <td>
            <c:Calendar ui:field="analysisStartedDate" begin="0" end="4" width="125px" pattern="{resource:getString($constants,'dateTimePattern')}"/>    
          </td> 
        </tr>
        <tr>
           <td class="{res.style.Prompt}"><ui:text from="{msg.status}" /></td>
           <td>
             <w:Dropdown ui:field="analysisStatusId" width="150px" popWidth="150px" field="Integer"/>
           </td>
           <td class="{res.style.Prompt}"><ui:text from="{msg.revision}" /></td> 
           <td>
             <w:Textbox ui:field="analysisRevision" width="60px" field="Integer" />
           </td>
           <td class="{res.style.Prompt}"><ui:text from="{msg.testReportable}" /></td>
           <td>
               <w:Checkbox ui:field="analysisIsReportable"/>
           </td>
           <td class="{res.style.Prompt}"><ui:text from="{msg.completed}" /></td>
           <td>
             <c:Calendar ui:field="analysisCompletedDate" begin="0" end="4" width="125px" pattern="{resource:getString($constants,'dateTimePattern')}"/>    
           </td>
         </tr>
         <tr>
           <td class="{res.style.Prompt}"><ui:text from="{msg.section}" /></td>
           <td>
             <w:Dropdown ui:field="analysisSectionId" width="151px" popWidth="150px" case="LOWER" field="Integer"/>
           </td>
           <td class="{res.style.Prompt}"><ui:text from="{msg.unit}" /></td>
           <td colspan="3">
              <w:Dropdown ui:field="analysisUnitOfMeasureId" width="150px" popWidth="150px" field="Integer"/>
           </td>
           <td class="{res.style.Prompt}"><ui:text from="{msg.released}" /></td>
           <td>
             <c:Calendar ui:field="analysisReleasedDate" begin="0" end="4" width="125px" pattern="{msg.dateTimePattern"/>    
           </td> 
       </tr>
       <tr>
          <td class="{res.style.Prompt}"><ui:text from="{msg.samplePrep}" /></td>
          <td colspan="5">
            <td>
             <w:AutoComplete ui:field="analysisSamplePrep" width="365x" case="LOWER">
               <w:popup>
                 <t:Table visibleRows="10" header="true">
                   <t:Column width="350px" label="{msg.name}">
                     <t:LabelCell />
                   </t:Column>               
                 </t:Table>
               </w:popup>
             </w:AutoComplete>
           </td>
          </td>
          <td class="{res.style.Prompt}"><ui:text from="{msg.printed}" /></td>
          <td>
             <c:Calendar ui:field="analysisPrintedDate" begin="0" end="4" width="125px" pattern="{msg.dateTimePattern}"/>    
          </td> 
        </tr>
      </table>
      <g:HorizontalPanel>
         <g:VerticalPanel>
            <table key="worksheetTable" style="ScreenTableWithSides" width="auto" tab = "analysisUserTable,{meta:getAnalysisCompletedDate()}" maxRows="3" showScroll="ALWAYS" title="">
            	<col width="70" header="Worksheet">
              		<label field="Integer"/>
            	</col>
            	<col width="115" header="Created">
              		<calendar width="115" begin="0" end="2" pattern="{resource:getString($constants,'dateTimePattern')}" />
            	</col>
            	<col width="115" header="Status">
              		<dropdown width="100" field="Integer" />
            	</col>
            	<col width="120" header="User">
              		<label field="String"/>
            	</col>
            </table>
                  <t:Table ui:field="worksheetTable" visibleRows="3" verticalScroll="ALWAYS" horizontalScroll="ALWAYS" header="true">
        <t:Column width="70" label="{msg.worksheet}">
          <t:LabelCell>
            <t:LabelCell field="Integer"/>
          </t:LabelCell>
        </t:Column>
        <t:Column width="115" label="{msg.contactName}">
          <t:TextBoxCell>
            <w:TextBox maxLength="40" required="true" />
          </t:TextBoxCell>
          calendar
        </t:Column>
        <t:Column width="100" label="{msg.workNumber}">
          <t:TextBoxCell>
            <w:TextBox maxLength="21" mask="{msg.phoneWithExtensionPattern}" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="90" label="{msg.homeNumber}">
          <t:TextBoxCell>
            <w:TextBox maxLength="16" mask="{msg.phonePattern}" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="90" label="{msg.cellNumber}">
          <t:TextBoxCell>
            <w:TextBox maxLength="16" mask="{msg.phonePattern}" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="90" label="{msg.faxNumber}">
          <t:TextBoxCell>
            <w:TextBox maxLength="16" mask="{msg.phonePattern}" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="150" label="{msg.email}">
          <t:TextBoxCell>
            <w:TextBox maxLength="80" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="130" label="{msg.aptSuite}">
          <t:TextBoxCell>
            <w:TextBox case="UPPER" maxLength="30" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="130" label="{msg.address}">
          <t:TextBoxCell>
            <w:TextBox case="UPPER" maxLength="40" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="130" sortable="true" label="{msg.city}">
          <t:TextBoxCell>
            <w:TextBox case="UPPER" maxLength="30" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="56" label="{msg.state}">
          <t:DropdownCell>
            <w:Dropdown width="40" case="UPPER" ui:field="state"/>
          </t:DropdownCell>
        </t:Column>
        <t:Column width="68" label="{msg.zipcode}">
          <t:TextBoxCell>
            <w:TextBox mask="99999-9999" />
          </t:TextBoxCell>
        </t:Column>
        <t:Column width="126" label="{msg.country}">
          <t:DropdownCell>
            <w:Dropdown width="110" field="String" ui:field="country"/>
          </t:DropdownCell>
        </t:Column>
      </t:Table>
	            <HorizontalPanel style="TableFooterPanel">
                 <appButton key="selectWkshtButton" style="Button">
                   <HorizontalPanel>
                     <AbsolutePanel style="selectIcon" />
                     <text>
                       <xsl:value-of select='resource:getString($constants,"select")' />
                     </text>
                   </HorizontalPanel>
                 </appButton>
           </HorizontalPanel>
        </g:VerticalPanel>
            <AbsolutePanel style="Divider" />
                  <VerticalPanel spacing="0" padding="0">
      		<table key="analysisUserTable" tab = "{meta:getAnalysisTestName()},worksheetTable" style="ScreenTableWithSides" width="auto" maxRows="3" showScroll="ALWAYS" title="">
            	<col width="115" header="User">
              		<autoComplete width="100" case="LOWER" field="Integer" required="true"/>
            	</col>
            	<col width="114" header="Action">
              		<dropdown width="100" field="Integer" required="true"/>
            	</col>
            </table>
            <HorizontalPanel style="TableFooterPanel">
               <appButton key="addActionButton" style="Button">
                        <HorizontalPanel>
                          <AbsolutePanel style="AddRowButtonImage" />
                          <text>
                            <xsl:value-of select="resource:getString($constants,'addRow')" />
                          </text>
                        </HorizontalPanel>
                      </appButton>
                      <appButton key="removeActionButton" style="Button">
                        <HorizontalPanel>
                          <AbsolutePanel style="RemoveRowButtonImage" />
                          <text>
                            <xsl:value-of select="resource:getString($constants,'removeRow')" />
                          </text>
                        </HorizontalPanel>
                      </appButton>
                  </HorizontalPanel>
                  </VerticalPanel>
      </g:HorizontalPanel>
    </g:VerticalPanel>
    </g:layer>
  </g:LayoutPanel>
</xsl:stylesheet>
