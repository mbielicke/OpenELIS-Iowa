<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
  xmlns:t='urn:import:org.openelis.ui.widget.table'>

  <ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
  <ui:with type="org.openelis.ui.resources.UIResources" field="libRes" />
  <ui:with type="org.openelis.modules.main.client.resources.OpenELISResources" field="res" />

  <g:SplitLayoutPanel styleName="{res.style.AppBackground}">
  <!-- 
    <g:west size="0">
      <g:VerticalPanel ui:field="favoritesPanel" styleName="{res.style.favoritesMenuContainer}" visible="true" width="100%" height="100%">
        <g:HorizontalPanel height="30px" styleName="{libRes.menuCss.topMenuBar}" width="100%">
          <g:Label styleName="{libRes.menuCss.topMenuBarItem}">Favorites</g:Label>
          <w:Button ui:field="editFavorites" leftIcon="{res.style.EditSettings}" />
        </g:HorizontalPanel>
      </g:VerticalPanel>
    </g:west>
  -->
    <g:center>
      <g:LayoutPanel>
        <g:layer top="0px" height="30px">
          <g:AbsolutePanel width="100%">
            <w:MenuBar>
              <w:Menu label="{msg.application}">
                <w:MenuItem description="{msg.preferenceDescription}" enabled="true" icon="{res.style.preferenceIcon}" ui:field="preference"
                  display="{msg.preference}" />
                <w:MenuItem description="" enabled="true" ui:field="logs" display="{msg.logs}"/>
                <w:MenuItem description="{msg.logoutDescription}" enabled="true" icon="{res.style.logoutIcon}" ui:field="logout"
                  display="{msg.logout}" />
              </w:Menu>
              <w:Menu label="{msg.label}">
                <w:MenuItem description="" enabled="false" icon="" ui:field="sampleLoginLabelReport" display="{msg.loginBarcode}" />
                <w:MenuItem description="" enabled="false" icon="" ui:field="sampleLoginLabelAdditionalReport" display="{msg.loginBarcodeAdd}" />
              </w:Menu>
              <w:Menu label="{msg.sample}">
                <w:MenuItem description="{msg.quickEntryDescription}" enabled="false" icon="{res.style.quickEntryIcon}" ui:field="quickEntry"
                  display="{msg.quickEntry}" />
                <w:MenuItem description="" enabled="false" icon="{res.style.QCIcon}" ui:field="verification"
                  display="{msg.verification}" />
                <w:MenuItem description="{msg.trackingDescription}" enabled="false" icon="{res.style.trackingIcon}" ui:field="tracking"
                  display="{msg.tracking}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.environmentalSampleLoginDescription}" enabled="false"
                  icon="{res.style.environmentalSampleLoginIcon}" ui:field="environmentalSampleLogin" display="{msg.environmentalSampleLogin}" />
                <w:MenuItem description="{msg.privateWellWaterSampleLoginDescription}" enabled="false"
                  icon="{res.style.privateWellWaterSampleLoginIcon}" ui:field="privateWellWaterSampleLogin" display="{msg.privateWellWaterSampleLogin}" />
                <w:MenuItem description="{msg.sdwisSampleLoginDescription}" enabled="false" icon="{res.style.sdwisSampleLoginIcon}"
                  ui:field="sdwisSampleLogin" display="{msg.sdwisSampleLogin}" />
                <w:MenuItem description="{msg.clinicalSampleLoginDescription}" enabled="false" ui:field="clinicalSampleLogin"
                  display="{msg.clinicalSampleLogin}" />
                <w:MenuItem description="{msg.newbornScreeningSampleLoginDescription}" enabled="false" ui:field="newbornScreeningSampleLogin"
                  display="{msg.newbornScreeningSampleLogin}" />
                <w:MenuItem description="{msg.animalSampleLoginDescription}" enabled="false" ui:field="animalSampleLogin"
                  display="{msg.animalSampleLogin}" />
                <w:MenuItem description="{msg.ptSampleLoginDescription}" enabled="false" ui:field="ptSampleLogin" display="{msg.ptSampleLogin}" />
                <w:MenuItem description="" enabled="false" icon="" ui:field="testSampleManager" display="Test Sample Manager" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.projectDescription}" enabled="false" icon="{res.style.projectIcon}" ui:field="project"
                  display="{msg.project}" />
                <w:MenuItem description="{msg.providerDescription}" enabled="false" icon="{res.style.providerIcon}" ui:field="provider"
                  display="{msg.provider}" />
                <w:MenuItem description="{msg.organizationDescription}" enabled="false" icon="{res.style.organizationIcon}"
                  ui:field="organization" display="{msg.organization}" />
              </w:Menu>
              <w:Menu label="{msg.analysis}">
                <w:MenuItem description="{msg.worksheetCreationDescription}" enabled="false" icon="{res.style.worksheetCreationIcon}"
                  ui:field="worksheetCreation" display="{msg.worksheetCreation}" />
                <w:MenuItem description="{msg.worksheetCompletionDescription}" enabled="false" icon="{res.style.worksheetCompletionIcon}"
                  ui:field="worksheetCompletion" display="{msg.worksheetCompletion}" />
                <w:MenuItem description="{msg.addOrCancelDescription}" enabled="false" icon="{res.style.addOrCancelIcon}"
                  ui:field="addOrCancel" display="{msg.manageMultipleSamples}" />
                <w:MenuItem description="{msg.reviewAndReleaseDescription}" enabled="false" icon="{res.style.reviewAndReleaseIcon}"
                  ui:field="reviewAndRelease" display="{msg.reviewAndRelease}" />
                <w:MenuItem description="{msg.toDoDescription}" enabled="false" icon="{res.style.toDoIcon}" ui:field="toDo"
                  display="{msg.toDo}" />
                <w:MenuItem description="{msg.labelForDescription}" enabled="false" icon="{res.style.labelForIcon}" ui:field="labelFor"
                  display="{msg.labelFor}" />
                <w:MenuItem description="{msg.storageDescription}" enabled="false" icon="{res.style.storageIcon}" ui:field="storage"
                  display="{msg.storage}" />
                <w:MenuItem description="{msg.QCDescription}" enabled="false" icon="QCIcon" ui:field="QC" display="{msg.QC}" />
                <w:MenuItem description="{msg.analyteParameterDescription}" enabled="false" icon="{res.style.QCIcon}" ui:field="analyteParameter"
                  display="{msg.analyteParameter}" />
              </w:Menu>
              <w:Menu label="{msg.inventoryOrder}">
                <w:MenuItem description="{msg.internalOrderDescription}" enabled="false" icon="{res.style.orderIcon}" ui:field="internalOrder"
                  display="{msg.internalOrder}" />
                <w:MenuItem description="{msg.vendorOrderDescription}" enabled="false" icon="{res.style.orderIcon}" ui:field="vendorOrder"
                  display="{msg.vendorOrder}" />
                <w:MenuItem description="{msg.sendoutOrderDescription}" enabled="false" icon="{res.style.orderIcon}" ui:field="sendoutOrder"
                  display="{msg.sendoutOrder}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.fillOrderDescription}" enabled="false" icon="{res.style.fillOrderIcon}" ui:field="fillOrder"
                  display="{msg.fillOrder}" />
                <w:MenuItem description="{msg.shippingDescription}" enabled="false" icon="{res.style.shippingIcon}" ui:field="shipping"
                  display="{msg.shipping}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.buildKitsDescription}" enabled="false" ui:field="buildKits" display="{msg.buildKits}" />
                <w:MenuItem description="{msg.inventoryTransferDescription}" enabled="false" ui:field="inventoryTransfer"
                  display="{msg.inventoryTransfer}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.inventoryReceiptDescription}" enabled="false" icon="{res.style.inventoryReceiptIcon}"
                  ui:field="inventoryReceipt" display="{msg.inventoryReceipt}" />
                <w:MenuItem description="{msg.inventoryAdjustmentDescription}" enabled="false" ui:field="inventoryAdjustment"
                  display="{msg.inventoryAdjustment}" />
                <w:MenuItem description="{msg.inventoryItemDescription}" enabled="false" icon="{res.style.inventoryIcon}"
                  ui:field="inventoryItem" display="{msg.inventoryItem}" />
              </w:Menu>
              <w:Menu label="{msg.report}">
                <w:Menu display="{msg.login}" enabled="true" showBelow="false">
                  <w:MenuItem enabled="false" ui:field="verificationReport" display="{msg.verificationReport}" />
                  <w:MenuItem enabled="false" ui:field="testRequestFormReport" display="{msg.TRFReport}" />
                  <w:MenuItem enabled="false" ui:field="orderRequestForm" display="{msg.orderRequestForm}" />
                  <w:MenuItem enabled="false" ui:field="holdRefuseOrganization" display="{msg.holdRefuseOrganization}" />
                </w:Menu>
                <w:Menu display="{msg.reference}" enabled="true" showBelow="false">
                  <w:MenuItem enabled="false" ui:field="testReport" display="{msg.testReport}" />
                  <w:MenuItem enabled="false" ui:field="billingReport" display="{msg.billingReport}" />
                </w:Menu>
                <w:Menu display="{msg.summary}" enabled="true" showBelow="false">
                  <w:MenuItem enabled="false" ui:field="sampleInhouseReport" display="{msg.sampleInhouseReport}" />
                  <w:MenuItem enabled="false" ui:field="volumeReport" display="{msg.volumeReport}" />
                  <w:MenuItem enabled="false" ui:field="toDoAnalyteReport" display="{msg.toDoAnalyteReport}" />
                  <w:MenuItem enabled="false" ui:field="sampleDataExport" display="{msg.sampleDataExport}" />
                  <w:MenuItem enabled="false" ui:field="QASummaryReport" display="{msg.QASummaryReport}" />
                  <w:MenuItem enabled="false" ui:field="testCountByFacility" display="{msg.testCountByFacility}" />
                  <w:MenuItem enabled="false" ui:field="turnaround" display="{msg.turnaround}" />
                  <w:MenuItem enabled="false" ui:field="turnAroundStatisticReport" display="{msg.turnAroundStatisticReport}" />
                  <w:MenuItem enabled="false" ui:field="sdwisUnloadReport" display="{msg.sdwisUnloadReport}" />
                  <w:MenuItem enabled="false" ui:field="dataView" display="{msg.dataView}" />
                  <w:MenuItem enabled="false" ui:field="qcChart" display="{msg.qcChart}" />
                  <w:MenuItem enabled="false" ui:field="finalReport" display="{msg.finalReport}" />
                  <w:MenuItem enabled="false" ui:field="orderRecurrence" display="{msg.orderRecurrence}" />
                  <w:MenuItem enabled="false" ui:field="finalReportBatch" display="{msg.finalReportBatch}" />
                  <w:MenuItem enabled="false" ui:field="finalReportBatchReprint" display="{msg.finalReportBatchReprint}" />
                </w:Menu>
              </w:Menu>
              <w:Menu label="{msg.maintenance}">
                <w:MenuItem description="{msg.testDescription}" enabled="false" icon="{res.style.testIcon}" ui:field="test"
                  display="{msg.test}" />
                <w:MenuItem description="{msg.methodDescription}" enabled="false" icon="{res.style.methodIcon}" ui:field="method"
                  display="{msg.method}" />
                <w:MenuItem description="{msg.panelDescription}" enabled="false" icon="{res.style.panelIcon}" ui:field="panel"
                  display="{msg.panel}" />
                <w:MenuItem description="{msg.QAEventDescription}" enabled="false" icon="{res.style.QAEventIcon}" ui:field="QAEvent"
                  display="{msg.QAEvent}" />
                <w:MenuItem description="{msg.labSectionDescription}" enabled="false" icon="{res.style.labSectionIcon}" ui:field="labSection"
                  display="{msg.labSection}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.analyteDescription}" enabled="false" icon="{res.style.analyteIcon}" ui:field="analyte"
                  display="{msg.analyte}" />
                <w:MenuItem description="{msg.dictionaryDescription}" enabled="false" icon="{res.style.dictionaryIcon}" ui:field="dictionary"
                  display="{msg.dictionary}" />
                <w:MenuItem description="{msg.auxiliaryPromptDescription}" enabled="false" icon="{res.style.auxiliaryPromptIcon}"
                  ui:field="auxiliaryPrompt" display="{msg.auxiliaryPrompt}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.exchangeVocabularyMapDescription}" enabled="false" icon="{res.style.dictionaryIcon}"
                  ui:field="exchangeVocabularyMap" display="{msg.exchangeVocabularyMap}" />
                <w:MenuItem description="{msg.exchangeDataSelectionDescription}" enabled="false" icon="{res.style.dictionaryIcon}"
                  ui:field="exchangeDataSelection" display="{msg.exchangeDataSelection}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.labelDescription}" enabled="false" icon="{res.style.labelIcon}" ui:field="label"
                  display="{msg.label}" />
                <w:MenuItem description="{msg.standardNoteDescription}" enabled="false" icon="{res.style.standardNoteIcon}"
                  ui:field="standardNote" display="{msg.standardNote}" />
                <w:MenuItem description="{msg.trailerForTestDescription}" enabled="false" icon="{res.style.trailerForTestIcon}"
                  ui:field="trailerForTest" display="{msg.trailerForTest}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.storageUnitDescription}" enabled="false" icon="{res.style.storageUnitIcon}"
                  ui:field="storageUnit" display="{msg.storageUnit}" />
                <w:MenuItem description="{msg.storageLocationDescription}" enabled="false" icon="{res.style.storageLocationIcon}"
                  ui:field="storageLocation" display="{msg.storageLocation}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.instrumentDescription}" enabled="false" icon="{res.style.instrumentIcon}" ui:field="instrument"
                  display="{msg.instrument}" />
                <g:HTML>
                  <hr />
                </g:HTML>
                <w:MenuItem description="{msg.scriptletDescription}" enabled="false" ui:field="scriptlet" display="{msg.scriptlet}" />
                <w:MenuItem description="{msg.systemVariableDescription}" enabled="false" icon="{res.style.systemVariableIcon}"
                  ui:field="systemVariable" display="{msg.systemVariable}" />
                <w:MenuItem description="{msg.pwsInformationDescription}" enabled="false" icon="{res.style.sdwisSampleLoginIcon}"
                  ui:field="pws" display="{msg.pwsInformation}" />
                <w:MenuItem description="{msg.cronDescription}" enabled="false" ui:field="cron" display="{msg.cron}" />
              </w:Menu>
            </w:MenuBar>
          </g:AbsolutePanel>
        </g:layer>
        <g:layer top="30px" bottom="0px">
          <w:Browser ui:field="browser" limit="10" />
        </g:layer>
      </g:LayoutPanel>
    </g:center>
  </g:SplitLayoutPanel>
</ui:UiBinder>
