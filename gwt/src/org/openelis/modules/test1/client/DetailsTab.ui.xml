<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:w='urn:import:org.openelis.ui.widget'
	xmlns:r='urn:import:org.openelis.ui.widget.tree' xmlns:t='urn:import:org.openelis.ui.widget.table'
	xmlns:c="urn:import:org.openelis.ui.widget.calendar" xmlns:o='urn:import:org.openelis.modules.test1.client'>

	<ui:with type="org.openelis.constants.OpenELISConstants" field="msg" />
	<ui:with type="org.openelis.modules.main.client.resources.OpenELISResources"
		field="res" />

	<g:VerticalPanel padding="0" spacing="0">
		<g:layer width="625px" height="515px">
			<g:HTMLPanel>
				<table class="{res.style.Form}">
					<tr>
						<td class="{res.style.Prompt}">
							<ui:text from="{msg.gen_description}:" />
						</td>
						<td>
							<w:TextBox ui:field="description" width="425px" max="60"
								field="String" required="true" />
						</td>
					</tr>
					<tr>
						<td class="{res.style.Prompt}">
							<ui:text from="{msg.test_reportDescription}:" />
						</td>
						<td>
							<w:TextBox ui:field="reportingDescription" width="425px"
								max="60" field="String" required="true" />
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
		</g:layer>
		<layer>
			<HorizontalPanel>
				<VerticalPanel style="subform">
					<td class="{res.style.Prompt}">
						<ui:text from="{msg.test_turnaround}:" />
					</td>
					<g:HTMLPanel>
						<table class="{res.style.Form}">
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_turnAroundMax}:" />
								</td>
								<td>
									<w:TextBox ui:field="timeTaMax" width="50px" field="Integer"
										required="true" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_timeTransit}:" />
								</td>
								<td>
									<w:TextBox ui:field="timeTransit" width="50px" field="Integer"
										required="true" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_turnAroundAverage}:" />
								</td>
								<td>
									<w:TextBox ui:field="timeTaAverage" width="50px"
										field="Integer" required="true" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_timeHolding}:" />
								</td>
								<td>
									<w:TextBox ui:field="timeHolding" width="50px" field="Integer"
										required="true" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_turnAroundWarn}:" />
								</td>
								<td>
									<w:TextBox ui:field="timeTaWarning" width="50px"
										field="Integer" required="true" />
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</VerticalPanel>
				<VerticalPanel style="subform">
					<td class="{res.style.Prompt}">
						<ui:text from="{msg.test_activity}:" />
					</td>
					<g:HTMLPanel>
						<table class="{res.style.Form}">
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_active}:" />
								</td>
								<td>
									<w:CheckBox ui:field="isActive" required="true" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_beginDate}:" />
								</td>
								<td>
									<c:Calendar ui:field="activeBegin" width="90px"
										required="true" begin="0" end="2" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_endDate}:" />
								</td>
								<td>
									<c:Calendar ui:field="activeEnd" width="90px"
										required="true" begin="0" end="2" />
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</VerticalPanel>
			</HorizontalPanel>
		</layer>
		<layer>
			<HorizontalPanel>
				<VerticalPanel style="subform">
					<td class="{res.style.Prompt}">
						<ui:text from="{msg.test_additionalLabel}:" />
					</td>
					<g:HTMLPanel>
						<table class="{res.style.Form}">
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_type}:" />
								</td>
								<td colspan="2">
									<w:AutoComplete ui:field="label" width="224px"
										field="Integer">
										<w:popup>
											<t:Table>
												<t:Column>
													<t:LabelCell />
												</t:Column>
											</t:Table>
										</w:popup>
									</w:AutoComplete>
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_quantity}:" />
								</td>
								<td>
									<w:TextBox ui:field="labelQty" width="50px" field="Integer" />
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</VerticalPanel>
				<VerticalPanel>
					<td class="{res.style.Prompt}">
						<ui:text from="{msg.test_sections}:" />
					</td>
					<t:Table ui:field="sectionTable" horizontalScroll="ALWAYS"
						verticalScroll="ALWAYS" width="119px" header="true"
						styleName="{res.table.ScreenTableWithSides}" maxRows="2">
						<t:Column width="119px" label="{msg.gen_name}" required="true">
							<t:DropdownCell>
								<w:Dropdown width="125" field="Integer" ui:field="sectionName" />
							</t:DropdownCell>
						</t:Column>
						<t:Column width="180" label="{msg.gen_options}">
							<t:DropdownCell>
								<w:Dropdown width="125" field="Integer" ui:field="sectionOption" />
							</t:DropdownCell>
						</t:Column>
					</t:Table>
				</VerticalPanel>
				<g:layer bottom="0px" height="25px">
					<g:HorizontalPanel>
						<w:Button ui:field="addSectionButton" leftIcon="{res.icon.addRowIcon}"
							text="{msg.gen_addRow}" />
						<w:Button ui:field="removeSectionButton" leftIcon="{res.icon.removeRowIcon}"
							text="{msg.gen_removeRow}" />
					</g:HorizontalPanel>
				</g:layer>
			</HorizontalPanel>
		</layer>
		<layer>
			<HorizontalPanel>
				<VerticalPanel>
					<g:HTMLPanel>
						<table class="{res.style.Form}">
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_reportable}:" />
								</td>
								<td>
									<w:CheckBox ui:field="isReportable" required="true" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_revisionMethod}:" />
								</td>
								<td>
									<w:Dropdown ui:field="revisionMethod" width="190px"
										field="Integer" required="true" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_testTrailer}:" />
								</td>
								<td colspan="2">
									<w:AutoComplete ui:field="testTrailer" width="180px"
										case="LOWER" field="Integer">
										<w:popup>
											<t:Table>
												<t:Column>
													<t:LabelCell />
												</t:Column>
											</t:Table>
										</w:popup>
									</w:AutoComplete>
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_sortingMethod}:" />
								</td>
								<td>
									<w:Dropdown ui:field="sortingMethod" width="190px"
										field="Integer" required="true" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_testFormat}:" />
								</td>
								<td>
									<w:Dropdown ui:field="testFormat" width="180px"
										field="Integer" required="true" />
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_reportingMethod}:" />
								</td>
								<td>
									<w:Dropdown ui:field="reportingMethod" width="190px"
										field="Integer" required="true" />
								</td>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.gen_scriptlet}:" />
								</td>
								<td colspan="2">
									<w:AutoComplete ui:field="scriptlet" width="180px"
										field="Integer">
										<w:popup>
											<t:Table>
												<t:Column>
													<t:LabelCell />
												</t:Column>
											</t:Table>
										</w:popup>
									</w:AutoComplete>
								</td>
							</tr>
							<tr>
								<td class="{res.style.Prompt}">
									<ui:text from="{msg.test_reportingSequence}:" />
								</td>
								<td>
									<w:TextBox ui:field="reportingSequence" width="80px"
										field="Integer" />
								</td>
							</tr>
						</table>
					</g:HTMLPanel>
				</VerticalPanel>
			</HorizontalPanel>
		</layer>
	</g:VerticalPanel>
</ui:UiBinder>