    <g:LayoutPanel>
      <g:layer top="0" height="30">
        <g:AbsolutePanel styleName="{uiRes.buttonPanel.ButtonPanelContainer}" width="100%">
            <g:HorizontalPanel>
              <w:Button css="{uiRes.buttonPanel}" ui:field="query" leftIcon="{uiRes.buttonPanel.QueryButtonImage}" text="{msg.query}"
