<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name="com.google.gwt.xml.XML"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name="org.openelis.gwt.Widget"/>
    <inherits name="org.openelis.gwt.Common"/>
    <inherits name="org.openelis.gwt.Screen"/>
    <inherits name="org.openelis.client.main.Main"/>
    <inherits name="org.openelis.client.utilities.Utilities"/>
    <inherits name="org.openelis.client.dataEntry.DataEntry"/>
    
	<!-- Specify the app entry point class.                   -->
	<entry-point class='org.openelis.client.main.OpenELIS'/>
      
    <servlet path='/OpenELISService' class='org.openelis.server.OpenELISServiceImpl'/>  
    <servlet path='/OpenELISScreen' class='org.openelis.server.OpenELISScreen'/>  
    <servlet path='/OrganizationScreen' class='org.openelis.server.OrganizationScreen'/>  
    <servlet path='/OrganizeFavoritesScreen' class='org.openelis.server.OrganizeFavoritesScreen'/>  
    <extend-property name="locale" values="cn"/>
   <!-- <extend-property name="locale" values="fa"/>
    <extend-property name="locale" values="sp"/> -->
</module>
