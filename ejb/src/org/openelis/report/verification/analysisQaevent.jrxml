<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sampleQaevent" language="groovy" pageWidth="722" pageHeight="572" orientation="Landscape" columnWidth="722" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ANALYSIS_ID" class="java.lang.Integer"/>
	<parameter name="BUFF" class="java.lang.StringBuffer"/>
	<queryString>
		<![CDATA[SELECT
     q.reporting_sequence, aq.id, q.reporting_text
FROM
     qaevent q, analysis_qaevent aq
WHERE
     aq.analysis_id = $P{ANALYSIS_ID}
     AND aq.qaevent_id = q.id
ORDER BY q.reporting_sequence, aq.id]]>
	</queryString>
	<field name="reporting_sequence" class="java.lang.Integer"/>
	<field name="id" class="java.lang.Integer"/>
	<field name="reporting_text" class="java.lang.String"/>
	<detail>
		<band>
			<textField isBlankWhenNull="true">
				<reportElement key="reportingText" x="0" y="0" width="722" height="0" isRemoveLineWhenBlank="true"/>
				<textElement>
					<font pdfFontName="Times-Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{BUFF}.append($F{reporting_text}).append(' ') ? null : null]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
