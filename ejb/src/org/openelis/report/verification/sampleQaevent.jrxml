<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sampleQaevent" language="groovy" pageWidth="702" pageHeight="572" orientation="Landscape" columnWidth="702" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="SAMPLE_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     q.reporting_sequence, sq.id, d.entry type, q.reporting_text
FROM
     qaevent q, sample_qaevent sq, dictionary d
WHERE
     sq.sample_id = $P{SAMPLE_ID}
     AND sq.qaevent_id = q.id
     AND q.type_id = d.id
ORDER BY q.reporting_sequence, sq.id]]>
	</queryString>
	<field name="reporting_sequence" class="java.lang.Integer"/>
	<field name="id" class="java.lang.Integer"/>
	<field name="type" class="java.lang.String"/>
	<field name="reporting_text" class="java.lang.String"/>
	<detail>
		<band height="12">
			<textField isBlankWhenNull="true">
				<reportElement key="reportingText" x="0" y="0" width="702" height="12" isRemoveLineWhenBlank="true"/>
				<textElement>
					<font pdfFontName="Times-Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["("+$F{type}.trim()+") - "+$F{reporting_text}.trim()]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
