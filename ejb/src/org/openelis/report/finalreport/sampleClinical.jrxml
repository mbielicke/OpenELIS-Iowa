	<queryString language="SQL">
		<![CDATA[SELECT
     s.received_date s_received_date, s.collection_date s_collection_date,
     s.collection_time s_collection_time, s.client_reference s_client_reference, s.released_date s_released_date,
     p.last_name pat_last, p.first_name pat_first, p.birth_date pat_birth_date, g.entry pat_gender, 
     pa.multiple_unit pat_multiple_unit, pa.street_address pat_street_address, pa.city pat_city, 
     pa.state pat_state, pa.zip_code pat_zip_code,
     pr.last_name pro_last, pr.first_name pro_first
FROM
     sample s
     join sample_clinical sc on s.id = sc.sample_id
     join patient p on sc.patient_id = p.id
     join address pa on p.address_id = pa.id
     left join dictionary g on p.gender_id = g.id
     left join provider pr on sc.provider_id = pr.id
WHERE
     s.domain = 'C' and s.id = $P{SAMPLE_ID}]]>
	</queryString>
