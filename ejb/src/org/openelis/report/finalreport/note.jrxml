<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="note" language="groovy" pageWidth="520" pageHeight="572" orientation="Landscape" columnWidth="520" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="REFERENCE_ID" class="java.lang.Integer"/>
	<parameter name="REFERENCE_TABLE_ID" class="java.lang.Integer"/>
	<parameter name="BUFF" class="java.lang.StringBuffer"/>
	<queryString>
		<![CDATA[SELECT
     note.text AS note_text
FROM
     note note
WHERE
     note.reference_id = $P{REFERENCE_ID}  AND  note.reference_table_id = $P{REFERENCE_TABLE_ID}
   AND  note.is_External = 'Y']]>
	</queryString>
	<field name="note_text" class="java.lang.String"/>
	<detail>
		<band splitType="Stretch">
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="0" width="520" height="0" isRemoveLineWhenBlank="true"/>
				<textElement>
					<font fontName="Times New Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{BUFF}.append($F{note_text}).append(' ') ? null : null]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
