<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="analysisQaevent" language="groovy" pageWidth="550" pageHeight="572" columnWidth="550" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ANALYSIS_ID" class="java.lang.Integer"/>
	<parameter name="BUFF" class="java.lang.StringBuffer"/>
	<queryString>
		<![CDATA[SELECT reporting_sequence, reporting_text
 FROM qaevent q, analysis_qaevent aq
 WHERE aq.analysis_id = $P{ANALYSIS_ID} and
       aq.qaevent_id = q.id and aq.type_id in (select id from dictionary
       where system_name in ('qaevent_override',  'qaevent_warning'))]]>
	</queryString>
	<field name="reporting_sequence" class="java.lang.Integer"/>
	<field name="reporting_text" class="java.lang.String"/>
	<detail>
		<band splitType="Stretch">
			<textField isBlankWhenNull="true">
				<reportElement x="0" y="0" width="520" height="0" isRemoveLineWhenBlank="true"/>
				<textElement>
					<font pdfFontName="Times-Roman"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{BUFF}.append($F{reporting_text}).append(' ')]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
